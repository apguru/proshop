{
  "extends": [
    "plugin:react/recommended",
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"

  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off",
    "no-case-declarations": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "warn",
    "no-nested-ternary": "off",
    "no-console": "off",
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ]
  }
}